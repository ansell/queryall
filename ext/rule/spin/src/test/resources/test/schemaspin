              </rdf:Description>
            </sp:object>
          </rdf:Description>
          <sp:Filter>
            <sp:expression>
              <sp:or>
                <sp:arg1>
                  <sp:lt>
                    <sp:arg1>
                      <xsd:double>
                        <sp:arg1 rdf:nodeID="A13"/>
                      </xsd:double>
                    </sp:arg1>
                    <sp:arg2 rdf:resource="http://spinrdf.org/spin#_arg2"/>
                  </sp:lt>
                </sp:arg1>
                <sp:arg2>
                  <sp:gt>
                    <sp:arg1>
                      <xsd:double>
                        <sp:arg1 rdf:nodeID="A13"/>
                      </xsd:double>
                    </sp:arg1>
                    <sp:arg2 rdf:resource="http://spinrdf.org/spin#_arg3"/>
                  </sp:gt>
                </sp:arg2>
              </sp:or>
            </sp:expression>
          </sp:Filter>
          <sp:Bind>
            <sp:variable rdf:nodeID="A4"/>
            <sp:expression>
              <fn:concat>
                <sp:arg1>
                  <spif:name>
                    <sp:arg1 rdf:resource="http://spinrdf.org/spin#_arg1"/>
                  </spif:name>
                </sp:arg1>
                <sp:arg2> must be between </sp:arg2>
                <sp:arg3 rdf:resource="http://spinrdf.org/spin#_arg2"/>
                <sp:arg4> and </sp:arg4>
